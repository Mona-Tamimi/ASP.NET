
@{
    ViewData["Title"] = "consultant";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Consultation Booking</title>
    <style>

        body {
            overflow-x: hidden;
        }
        /* ************************************ */
        .img {
            width: 50vw;
            height: 30vw;
            box-shadow: 0.1vw 0.1vw 2vw #ECDFCF;
            transform: scale(0.9);
            transition: transform 0.3s ease-in-out;
        }

            .img:hover {
                transform: scale(1);
            }

        .hero {
            width: 100%;
            height: 15vw;
            background-color: rgba(128, 129, 198, 0.5);
            display: flex;
            justify-content: center;
            align-items: center;
            flex-direction: column;
            text-align: center;
            animation: fadeIn 2s ease-in-out;
        }

      

        h1,
        h2, h3 {
            color: #2F1893;
        }

        h1 {
            font-size: 3vw;
            animation: textGlow 2s infinite alternate;
        }

        /* ************************************ */



        .specialist-card {
            box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
            border-radius: 8px;
            overflow: hidden;
            background-color: #FFFFFF;
            margin-bottom: 20px;
            transition: box-shadow 0.3s ease;
        }

            .specialist-card:hover {
                box-shadow: rgba(17, 17, 26, 0.1) 0px 8px 24px, rgba(17, 17, 26, 0.1) 0px 16px 56px, rgba(17, 17, 26, 0.1) 0px 24px 80px;
            }

        .s31 {
            display: flex;
            justify-content: center;
            align-items: center;
            flex-direction: column;
            padding-left: 2vw;
        }

        .specialist-card img {
            width: 100%;
            height: 300px;
            object-fit: cover;
            transform: scale(1);
        }

            .specialist-card img:hover {
                transform: scale(1.1);
            }

        .specialist-card-body {
            padding: 15px;
        }

            .specialist-card-body h3 {
                color: #2F1893;
            }

            .specialist-card-body p {
                color: #5774EA;
            }

        .pagination {
            justify-content: center;
        }

        .btn-primary {
            background-color: #5774EA;
            border: none;
        }

            .btn-primary:hover {
                background-color: #2F1893;
            }

        .filter {
            display: flex;
            flex-direction: column;
        }

        .filter-options {
            align-items: center;
            justify-content: center;
            display: flex;
            flex-direction: row;
            gap: 10px;
        }

        .filter-option {
            padding: 10px;
            background-color: #f0f0f0;
            border: 1px solid #ddd;
            cursor: pointer;
            text-align: center;
            border-radius: 4px;
        }

            .filter-option:hover {
                background-color: #e0e0e0;
            }

            .filter-option.selected {
                background-color: #5774EA;
                color: white;
            }

        .pagination_btn {
            margin: 10px;
            cursor: pointer;
            padding: 10px 20px;
            border: none;
            background-color: #7F81C5;
            color: white;
            transform: scale(1);
        }

            .pagination_btn:hover {
                transform: scale(1.1);
                box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
                background-color: #7F81C5;
                color: white;
            }
    </style>
</head>
